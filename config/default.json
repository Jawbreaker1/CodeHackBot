{
  "agent": {
    "name": "BirdHackBot",
    "model": "qwen/qwen3-coder-30b",
    "system_prompt": "prompts/system.md",
    "max_steps": 6,
    "tool_max_fixes": 2,
    "tool_max_files": 20,
    "worker_assist_mode": "degraded"
  },
  "llm": {
    "base_url": "http://192.168.50.212:1234/v1,http://192.168.50.213:1234/v1",
    "model": "qwen/qwen3-coder-30b",
    "timeout_seconds": 120,
    "api_key": "",
    "max_failures": 3,
    "cooldown_seconds": 300,
    "temperature": 0.15,
    "max_tokens": 1200,
    "roles": {
      "assist": {
        "temperature": 0.15,
        "max_tokens": 1200
      },
      "planner": {
        "temperature": 0.05,
        "max_tokens": 2800
      },
      "summarize": {
        "temperature": 0.1,
        "max_tokens": 1000
      },
      "recovery": {
        "temperature": 0.1,
        "max_tokens": 900
      },
      "tui_assistant": {
        "temperature": 0.1,
        "max_tokens": 600
      }
    }
  },
  "permissions": {
    "level": "default",
    "require_approval": true
  },
  "scope": {
    "networks": ["internal"],
    "targets": [],
    "deny_targets": []
  },
  "context": {
    "max_recent_outputs": 20,
    "summarize_every_steps": 8,
    "summarize_at_percent": 70,
    "chat_history_lines": 60,
    "playbook_max": 2,
    "playbook_lines": 60
  },
  "session": {
    "ledger_enabled": false,
    "log_dir": "sessions",
    "plan_filename": "plan.md",
    "inventory_filename": "inventory.md",
    "ledger_filename": "ledger.md"
  },
  "tools": {
    "shell": {
      "enabled": true,
      "timeout_seconds": 1200
    },
    "metasploit": {
      "discovery_mode": "msfconsole",
      "rpc_enabled": false
    }
  },
  "network": {
    "assume_offline": true
  },
  "ui": {
    "verbose": false
  }
}
